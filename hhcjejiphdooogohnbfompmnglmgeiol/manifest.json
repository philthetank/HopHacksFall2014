{
   "background": {
      "scripts": [ "framework/background.js" ]
   },
   "browser_action": {
      "default_icon": "assets/img/WF128.png",
      "default_popup": "app/options.html",
      "default_title": "Word Filter"
   },
   "content_scripts": [ {
      "js": [ "framework/WordFilter.js" ],
      "matches": [ "http://*/*", "https://*/*", "file:///*/*" ],
      "run_at": "document_end"
   } ],
   "description": "Fix the web one word at a time.",
   "manifest_version": 2,
   "name": "Word Filter",
   "options_page": "app/options.html",
   "permissions": [ "http://*/*", "https://*/*" ],
   "version": "7.3"
}
